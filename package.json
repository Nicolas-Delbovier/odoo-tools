{
  "name": "odoo-tools",
  "displayName": "odoo-tools",
  "description": "Set of useful tools for odoo development",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nicolas-Delbovier/odoo-tools"
  },
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "medias/icons/odoo-tools-icon.png",
  "publisher": "nicolas-delbovier", 
  "author": "Nicolas Delbovier",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "odoo-tools.copyAddonName",
        "title": "Odoo-tools: Copy Addon Name"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "odoo-tools.copyAddonName",
          "group": "odoo-tools"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
